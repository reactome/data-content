<?xml version="1.0" encoding="UTF-8" ?>
<taglib
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
        version="2.1">

    <display-name>Database Object Functions</display-name>
    <tlib-version>1.0</tlib-version>
    <short-name>do</short-name>

    <function>
        <name>sortByDisplayName</name>
        <function-class>org.reactome.server.util.tags.DatabaseObjectSort</function-class>
        <function-signature>java.util.List sortByDisplayName(java.util.List)</function-signature>
    </function>

    <function>
        <name>sortList</name>
        <function-class>org.reactome.server.util.tags.DatabaseObjectSort</function-class>
        <function-signature>java.util.List sortList(java.util.List)</function-signature>
    </function>

    <function>
        <description>Join elements of an Iterable into a string.</description>
        <display-name>Join</display-name>
        <name>join</name>
        <function-class>org.reactome.server.util.StringUtils</function-class>
        <function-signature>java.lang.String join(java.lang.Iterable, java.lang.CharSequence)</function-signature>
    </function>

</taglib>